[server]
host = "0.0.0.0"
port = 8888
delete_old_data = true # Delete old data from database
delete_old_data_after = 30 # Delete data older than 30 days

[database]
database = "job-agg-db" # Database name
collection = "jobs-collections" # Collection name
document = "jobs-document" # Document name

[firebase]
url = "" # Firebase URL
credentials = "firebase.json" # Firebase credentials file name

[mongo]
local_uri = "mongodb://localhost:27017" # For local MongoDB
docker_uri = "mongodb://mongo:27017" # For Docker MongoDB

[search]
job_title = ["Software Engineer", "Backend Engineer"]
job_level = ["Entry Level", "New Grad"]